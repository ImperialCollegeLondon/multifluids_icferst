  <problem_definition length="medium" nprocs="1">
    <command_line>fluidity -v2 -l viscous_fs_topbottom.flml</command_line>
  </problem_definition>
  <variables>
    <variable name="freesurfacemaxend" language="python">from fluidity_tools import stat_parser as stat
