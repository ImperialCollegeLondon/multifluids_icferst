          </surface_ids>
          <type name="dirichlet">
            <apply_weakly/>
            <constant>
              <real_value rank="0">1</real_value>
            </constant>
          </type>
        </boundary_conditions>
        <detectors>
          <include_in_detectors/>
        </detectors>
        <consistent_interpolation/>
      </prognostic>
    </scalar_field>
    <multiphase_properties>
      <immobile_fraction>
        <scalar_field name="value">
          <prescribed>
            <mesh name="P0DG"/>
            <value name="WholeMesh">
              <constant>
                <real_value rank="0">0.2</real_value>
              </constant>
            </value>
            <stat/>
            <detectors>
              <exclude_from_detectors/>
            </detectors>
          </prescribed>
        </scalar_field>
      </immobile_fraction>
    </multiphase_properties>
  </material_phase>
  <material_phase name="phase2">
