          <algorithm name="scalar_python_diagnostic" material_phase_support="single">
            <string_value type="code" lines="20" language="python">p = state.tensor_fields["PackedCVPressure"]
bd = state.tensor_fields["PackedBathymetry"]

A = 0.101936799

for i in range( p.node_count ):
 P = p.node_val( i )
 B = bd.node_val( i )
 val = A*P[0][0]-B[0][0]
 field.set( i, val )</string_value>
