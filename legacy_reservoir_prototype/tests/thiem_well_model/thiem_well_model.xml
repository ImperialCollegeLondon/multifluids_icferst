<?xml version='1.0' encoding='utf-8'?>
<testproblem>
  <name>thiem_well_model</name>
  <owner userid="dimitrios.pavlidis"/>
  <tags>mpml this</tags>
  <problem_definition length="short" nprocs="1">
    <command_line>
../../../bin/multiphase_prototype thiem_well_model.mpml
  </command_line>
  </problem_definition>
  <variables>
    <variable name="phase1_pressure_max" language="python">from fluidity_tools import stat_parser as stat
phase1_pressure_max = stat("twm.stat")["phase1"]["Pressure"]["max"][-1]</variable>
    <variable name="phase1_pressure_min" language="python">from fluidity_tools import stat_parser as stat
phase1_pressure_min = stat("twm.stat")["phase1"]["Pressure"]["min"][-1]</variable>
    <variable name="phase2_pressure_max" language="python">from fluidity_tools import stat_parser as stat
phase2_pressure_max = stat("twm.stat")["phase2"]["Pressure"]["max"][-1]</variable>
    <variable name="phase2_pressure_min" language="python">from fluidity_tools import stat_parser as stat
phase2_pressure_min = stat("twm.stat")["phase2"]["Pressure"]["min"][-1]</variable>
    <variable name="phase1_pressure_max_ref" language="python">
phase1_pressure_max_ref = 1000066.28719</variable>
    <variable name="phase1_pressure_min_ref" language="python">
phase1_pressure_min_ref = 939932.294612</variable>
    <variable name="phase2_pressure_max_ref" language="python">
phase2_pressure_max_ref = 923268.544617</variable>
    <variable name="phase2_pressure_min_ref" language="python">
phase2_pressure_min_ref = 0.0</variable>
    <variable name="tolerance" language="python">
tolerance = 1.0e-5</variable>
  </variables>
  <pass_tests>
    <test name="phase 1 max pressure check" language="python">assert( abs(phase1_pressure_max-phase1_pressure_max_ref) &lt; tolerance )</test>
    <test name="phase 1 min pressure check" language="python">assert( abs(phase1_pressure_min-phase1_pressure_min_ref) &lt; tolerance )</test>
    <test name="phase 2 max pressure check" language="python">assert( abs(phase2_pressure_max-phase2_pressure_max_ref) &lt; tolerance )</test>
    <test name="phase 2 min pressure check" language="python">assert( abs(phase2_pressure_min-phase2_pressure_min_ref) &lt; tolerance )</test>
  </pass_tests>
  <warn_tests/>
</testproblem>
