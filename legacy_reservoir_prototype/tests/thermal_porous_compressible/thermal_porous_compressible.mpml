              <string_value lines="20" type="code" language="python">def val(X, t):
 val =10132500.0 + 9.81*1000
 return val</string_value>
            </python>
          </type>
        </boundary_conditions>
        <consistent_interpolation/>
      </prognostic>
    </scalar_field>
