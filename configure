static long int longval () { return "PETSC_VERSION_MAJOR"; }
static unsigned long int ulongval () { return "PETSC_VERSION_MAJOR"; }
#include <stdio.h>
#include <stdlib.h>
#ifdef F77_DUMMY_MAIN

#  ifdef __cplusplus
     extern "C"
#  endif
   int F77_DUMMY_MAIN() { return 1; }

