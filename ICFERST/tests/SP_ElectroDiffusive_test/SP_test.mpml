<?xml version='1.0' encoding='utf-8'?>
<icferst_options>
  <simulation_name>
    <string_value lines="1">SP_test</string_value>
  </simulation_name>
  <porous_media_simulator/>
  <geometry>
    <dimension>
      <integer_value rank="0">2</integer_value>
    </dimension>
    <mesh name="CoordinateMesh">
      <from_file file_name="structured_mesh">
        <format name="gmsh"/>
        <stat>
          <include_in_stat/>
        </stat>
      </from_file>
    </mesh>
    <simulation_quality/>
  </geometry>
  <solver_options>
    <Non_Linear_Solver>
      <integer_value rank="0">2</integer_value>
    </Non_Linear_Solver>
  </solver_options>
  <io>
    <dump_format>
      <string_value>vtk</string_value>
    </dump_format>
    <dump_period_in_timesteps>
      <constant>
        <integer_value rank="0">1</integer_value>
      </constant>
    </dump_period_in_timesteps>
    <Generate_stat_file/>
  </io>
  <timestepping>
    <current_time>
      <real_value rank="0">0</real_value>
    </current_time>
    <timestep>
      <real_value rank="0">2592000</real_value>
    </timestep>
    <finish_time>
      <real_value rank="0">78840000</real_value>
      <comment>78840000 = 2.5 years</comment>
    </finish_time>
  </timestepping>
  <material_phase name="phase1">
    <phase_properties>
      <Density>
        <compressible>
          <Linear_eos>
            <reference_density>
              <real_value rank="0">1000</real_value>
            </reference_density>
            <alpha>
              <real_value rank="0">0.001461</real_value>
            </alpha>
          </Linear_eos>
        </compressible>
        <boundary_conditions name="all">
          <surface_ids>
            <integer_value rank="1" shape="4">8 7 9 10</integer_value>
          </surface_ids>
          <type name="dirichlet">
            <apply_weakly/>
            <constant>
              <real_value rank="0">1000</real_value>
            </constant>
          </type>
        </boundary_conditions>
      </Density>
      <Viscosity>
        <tensor_field name="Viscosity" rank="2">
          <prescribed>
            <mesh name="PressureMesh"/>
            <value name="WholeMesh">
              <isotropic>
                <constant>
                  <real_value rank="0">1e-3</real_value>
                </constant>
              </isotropic>
            </value>
          </prescribed>
        </tensor_field>
        <viscosity_scheme>
          <tensor_form/>
          <nonlinear_scheme/>
        </viscosity_scheme>
      </Viscosity>
    </phase_properties>
    <scalar_field rank="0" name="Pressure">
      <prognostic>
        <mesh name="PressureMesh"/>
        <initial_condition name="WholeMesh">
          <constant>
            <real_value rank="0">0</real_value>
          </constant>
        </initial_condition>
        <boundary_conditions name="outlet">
          <surface_ids>
            <integer_value rank="1" shape="1">8</integer_value>
          </surface_ids>
          <type name="dirichlet">
            <apply_weakly/>
            <constant>
              <real_value rank="0">14.3e6</real_value>
            </constant>
          </type>
        </boundary_conditions>
        <boundary_conditions name="inlet">
          <surface_ids>
            <integer_value rank="1" shape="1">7</integer_value>
          </surface_ids>
          <type name="dirichlet">
            <apply_weakly/>
            <constant>
              <real_value rank="0">14.65e6</real_value>
            </constant>
          </type>
        </boundary_conditions>
        <detectors>
          <exclude_from_detectors/>
        </detectors>
        <no_interpolation/>
        <output/>
      </prognostic>
    </scalar_field>
    <vector_field rank="1" name="Velocity">
      <prognostic>
        <mesh name="VelocityMesh"/>
        <initial_condition name="WholeMesh">
          <constant>
            <real_value rank="1" dim1="dim" shape="2">0 0</real_value>
          </constant>
        </initial_condition>
        <boundary_conditions name="up">
          <surface_ids>
            <integer_value rank="1" shape="1">9</integer_value>
          </surface_ids>
          <type name="dirichlet">
            <apply_weakly/>
            <align_bc_with_cartesian>
              <x_component>
                <constant>
                  <real_value rank="0">0</real_value>
                </constant>
              </x_component>
              <y_component>
                <constant>
                  <real_value rank="0">0</real_value>
                </constant>
              </y_component>
            </align_bc_with_cartesian>
          </type>
        </boundary_conditions>
        <boundary_conditions name="down">
          <surface_ids>
            <integer_value rank="1" shape="1">10</integer_value>
          </surface_ids>
          <type name="dirichlet">
            <apply_weakly/>
            <align_bc_with_cartesian>
              <x_component>
                <constant>
                  <real_value rank="0">0</real_value>
                </constant>
              </x_component>
              <y_component>
                <constant>
                  <real_value rank="0">0</real_value>
                </constant>
              </y_component>
            </align_bc_with_cartesian>
          </type>
        </boundary_conditions>
        <detectors>
          <include_in_detectors/>
        </detectors>
        <consistent_interpolation/>
        <output/>
      </prognostic>
    </vector_field>
    <scalar_field rank="0" name="PhaseVolumeFraction">
      <prognostic>
        <mesh name="PressureMesh"/>
        <initial_condition name="WholeMesh">
          <constant>
            <real_value rank="0">0.2</real_value>
          </constant>
        </initial_condition>
        <boundary_conditions name="inlet">
          <surface_ids>
            <integer_value rank="1" shape="1">7</integer_value>
          </surface_ids>
          <type name="dirichlet">
            <apply_weakly/>
            <constant>
              <real_value rank="0">1</real_value>
            </constant>
          </type>
        </boundary_conditions>
        <detectors>
          <include_in_detectors/>
        </detectors>
        <consistent_interpolation/>
      </prognostic>
    </scalar_field>
    <scalar_field rank="0" name="Concentration">
      <prognostic>
        <mesh name="PressureMesh"/>
        <Impose_min_max>
          <automatic_min_limit/>
          <automatic_max_limit/>
        </Impose_min_max>
        <initial_condition name="WholeMesh">
          <constant>
            <real_value rank="0">0.001</real_value>
          </constant>
        </initial_condition>
        <boundary_conditions name="inlet">
          <surface_ids>
            <integer_value rank="1" shape="1">7</integer_value>
          </surface_ids>
          <type name="dirichlet">
            <apply_weakly/>
            <constant>
              <real_value rank="0">0.0005</real_value>
            </constant>
          </type>
        </boundary_conditions>
        <detectors>
          <include_in_detectors/>
        </detectors>
        <consistent_interpolation/>
      </prognostic>
    </scalar_field>
    <scalar_field rank="0" name="Dummy">
      <diagnostic>
        <algorithm name="Internal" material_phase_support="multiple"/>
        <mesh name="PressureMesh"/>
        <stat/>
        <convergence>
          <include_in_convergence/>
        </convergence>
        <detectors>
          <include_in_detectors/>
        </detectors>
        <steady_state>
          <include_in_steady_state/>
        </steady_state>
      </diagnostic>
    </scalar_field>
    <multiphase_properties>
      <Relperm_Corey>
        <scalar_field name="relperm_max">
          <prescribed>
            <mesh name="P0DG"/>
            <value name="WholeMesh">
              <constant>
                <real_value rank="0">0.3</real_value>
              </constant>
            </value>
            <stat/>
            <detectors>
              <exclude_from_detectors/>
            </detectors>
          </prescribed>
        </scalar_field>
        <scalar_field name="relperm_exponent">
          <prescribed>
            <mesh name="P0DG"/>
            <value name="WholeMesh">
              <constant>
                <real_value rank="0">2</real_value>
              </constant>
            </value>
            <stat/>
            <detectors>
              <exclude_from_detectors/>
            </detectors>
          </prescribed>
        </scalar_field>
      </Relperm_Corey>
      <immobile_fraction>
        <scalar_field name="value">
          <prescribed>
            <mesh name="P0DG"/>
            <value name="WholeMesh">
              <constant>
                <real_value rank="0">0.2</real_value>
              </constant>
            </value>
            <stat/>
            <detectors>
              <exclude_from_detectors/>
            </detectors>
          </prescribed>
        </scalar_field>
      </immobile_fraction>
    </multiphase_properties>
  </material_phase>
  <porous_media>
    <scalar_field name="Porosity">
      <prescribed>
        <mesh name="P0DG"/>
        <value name="WholeMesh">
          <constant>
            <real_value rank="0">0.25</real_value>
          </constant>
        </value>
        <stat/>
        <detectors>
          <exclude_from_detectors/>
        </detectors>
        <output/>
      </prescribed>
    </scalar_field>
    <tensor_field name="Permeability">
      <prescribed>
        <mesh name="P0DG"/>
        <value name="WholeMesh">
          <diagonal>
            <constant>
              <real_value rank="1" dim1="dim" shape="2">9.869233e-14 9.869233e-15</real_value>
            </constant>
          </diagonal>
        </value>
        <output/>
      </prescribed>
    </tensor_field>
    <SelfPotential>
      <scalar_field name="SelfPotential">
        <diagnostic>
          <mesh name="PressureMesh"/>
          <algorithm name="Internal" material_phase_support="multiple"/>
          <stat/>
          <convergence>
            <include_in_convergence/>
          </convergence>
          <detectors>
            <include_in_detectors/>
          </detectors>
          <steady_state>
            <include_in_steady_state/>
          </steady_state>
          <output/>
        </diagnostic>
      </scalar_field>
      <reference_coordinates>
        <real_value rank="1" dim1="dim" shape="2">200 50</real_value>
      </reference_coordinates>
      <Reservoir_temperature>
        <real_value rank="0">343</real_value>
      </Reservoir_temperature>
      <python_Rock_sat_conductivity_code>
        <algorithm name="scalar_python_diagnostic">
          <string_value type="code" language="python" lines="20">for i in range( field.node_count ):
 val = 1.#We disable rock sat conductivity
 field.set( i, val )</string_value>
        </algorithm>
      </python_Rock_sat_conductivity_code>
      <python_ElectroKinetic_code>
        <algorithm name="scalar_python_diagnostic">
          <string_value type="code" language="python" lines="20">for i in range( field.node_count ):
 val = 0.#We disable EK for this test case
 field.set( i, val )</string_value>
        </algorithm>
      </python_ElectroKinetic_code>
    </SelfPotential>
  </porous_media>
</icferst_options>
