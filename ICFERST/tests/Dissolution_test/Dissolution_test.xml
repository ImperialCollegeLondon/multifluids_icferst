<?xml version='1.0' encoding='utf-8'?>
<testproblem>
  <name>Dissolution_test</name>
  <owner userid="pablo.salinas"/>
  <tags>porous mpml dissolution</tags>
  <problem_definition length="short" nprocs="1">
    <command_line>../../../bin/icferst Dissolution_test.mpml</command_line>
  </problem_definition>
  <variables>
    <variable name="max_val" language="python">from fluidity_tools import stat_parser as stat
max_val = stat("Drainage_test.stat")["water"]["Concentration"]["max"][-1]</variable>
    <variable name="min_val" language="python">from fluidity_tools import stat_parser as stat
min_val = stat("Drainage_test.stat")["water"]["Concentration"]["min"][-1]</variable>
    <variable name="integral_val" language="python">from fluidity_tools import stat_parser as stat
integral_val = stat("Drainage_test.stat")["water"]["Concentration"]["integral"][-1]</variable>
  </variables>
  <pass_tests>
    <test name="Assert final max concentration" language="python">assert(abs((max_val - 0.0340742)/0.0340742) &lt; 0.01)</test>
    <test name="Assert final min concentration" language="python">assert(abs((min_val - 0.031063)/0.031063) &lt; 0.01)</test>
    <test name="Assert final integral concentration" language="python">assert(abs((integral_val - 0.00433779)/0.00433779) &lt; 0.01)</test>
  </pass_tests>
  <warn_tests/>
</testproblem>
