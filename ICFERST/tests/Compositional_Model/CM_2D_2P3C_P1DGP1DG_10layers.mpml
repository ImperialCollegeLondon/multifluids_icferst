        <algorithm name="scalar_python_diagnostic" material_phase_support="single">
          <string_value lines="20" type="code" language="python">Xp = state.vector_fields["DiagnosticCoordinate"]
f = open( "2P3C_SatGas.dat", "r" )
xref=[] ; yref=[]
for line in f:
 d = [ float(i) for i in line.split() ]
 xref.append(d[0]) ; yref.append(d[1])
 
for i in range( field.node_count ):
 X = Xp.node_val(i) ; x = X[0]
 for j in range( len( xref ) - 1 ):
  if ( x&gt;=xref[j] and x&lt;=xref[j+1] ):
   a = ( yref[j+1]-yref[j] ) / ( xref[j+1] - xref[j] )
   b = yref[j] - a * xref[j]
   ref = a * x + b
   break

 field.set( i, ref )</string_value>
        </algorithm>
