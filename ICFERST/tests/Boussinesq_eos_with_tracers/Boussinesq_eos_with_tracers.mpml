<?xml version='1.0' encoding='utf-8'?>
<icferst_options>
  <simulation_name>
    <string_value lines="1">Boussinesq_eos_with_tracers</string_value>
  </simulation_name>
  <porous_media_simulator/>
  <geometry>
    <dimension>
      <integer_value rank="0">2</integer_value>
    </dimension>
    <mesh name="CoordinateMesh">
      <from_file file_name="structured_mesh">
        <format name="gmsh"/>
        <stat>
          <include_in_stat/>
        </stat>
      </from_file>
    </mesh>
    <simulation_quality>
      <string_value>precision</string_value>
    </simulation_quality>
  </geometry>
  <solver_options>
    <Linear_solver>
      <iterative_method name="gmres">
        <restart>
          <integer_value rank="0">30</integer_value>
        </restart>
      </iterative_method>
      <preconditioner name="hypre">
        <hypre_type name="boomeramg"/>
      </preconditioner>
      <relative_error>
        <real_value rank="0">1e-10</real_value>
      </relative_error>
      <max_iterations>
        <integer_value rank="0">300</integer_value>
      </max_iterations>
      <ignore_all_solver_failures/>
      <diagnostics>
        <monitors/>
      </diagnostics>
      <Custom_solver_configuration>
        <Pressure>
          <iterative_method name="gmres">
            <restart>
              <integer_value rank="0">30</integer_value>
            </restart>
          </iterative_method>
          <preconditioner name="hypre">
            <hypre_type name="boomeramg"/>
          </preconditioner>
          <relative_error>
            <real_value rank="0">1e-10</real_value>
          </relative_error>
          <max_iterations>
            <integer_value rank="0">300</integer_value>
          </max_iterations>
          <remove_null_space/>
          <ignore_all_solver_failures/>
          <diagnostics>
            <monitors/>
          </diagnostics>
        </Pressure>
      </Custom_solver_configuration>
    </Linear_solver>
  </solver_options>
  <io>
    <dump_format>
      <string_value>vtk</string_value>
    </dump_format>
    <dump_period_in_timesteps>
      <constant>
        <integer_value rank="0">1</integer_value>
      </constant>
    </dump_period_in_timesteps>
  </io>
  <timestepping>
    <current_time>
      <real_value rank="0">0</real_value>
    </current_time>
    <timestep>
      <real_value rank="0">1</real_value>
    </timestep>
    <finish_time>
      <real_value rank="0">1</real_value>
    </finish_time>
  </timestepping>
  <physical_parameters>
    <gravity>
      <magnitude>
        <real_value rank="0">9.81</real_value>
      </magnitude>
      <vector_field name="GravityDirection" rank="1">
        <prescribed>
          <mesh name="CoordinateMesh"/>
          <value name="WholeMesh">
            <constant>
              <real_value shape="2" dim1="dim" rank="1">-1 0</real_value>
            </constant>
          </value>
          <stat>
            <include_in_stat/>
          </stat>
          <detectors>
            <exclude_from_detectors/>
          </detectors>
        </prescribed>
      </vector_field>
    </gravity>
  </physical_parameters>
  <material_phase name="phase1">
    <phase_properties>
      <Density>
        <compressible>
          <Boussinesq_eos>
            <reference_density>
              <real_value rank="0">1</real_value>
            </reference_density>
            <alpha>
              <real_value rank="0">3e-2</real_value>
            </alpha>
            <scalar_field name="PassiveTracer_Example2">
              <R0>
                <real_value rank="0">0.1</real_value>
              </R0>
              <Coef>
                <real_value rank="0">3e-2</real_value>
              </Coef>
            </scalar_field>
          </Boussinesq_eos>
          <Boussinesq_approximation/>
        </compressible>
      </Density>
      <Viscosity>
        <tensor_field name="Viscosity" rank="2">
          <prescribed>
            <mesh name="PressureMesh"/>
            <value name="WholeMesh">
              <isotropic>
                <constant>
                  <real_value rank="0">1</real_value>
                </constant>
              </isotropic>
            </value>
          </prescribed>
        </tensor_field>
        <viscosity_scheme>
          <tensor_form/>
          <nonlinear_scheme/>
        </viscosity_scheme>
      </Viscosity>
    </phase_properties>
    <scalar_field name="Pressure" rank="0">
      <prognostic>
        <mesh name="PressureMesh"/>
        <initial_condition name="WholeMesh">
          <constant>
            <real_value rank="0">0</real_value>
          </constant>
        </initial_condition>
        <detectors>
          <exclude_from_detectors/>
        </detectors>
        <no_interpolation/>
        <output/>
      </prognostic>
    </scalar_field>
    <vector_field name="Velocity" rank="1">
      <prognostic>
        <mesh name="VelocityMesh"/>
        <initial_condition name="WholeMesh">
          <constant>
            <real_value shape="2" dim1="dim" rank="1">0 0</real_value>
          </constant>
        </initial_condition>
        <boundary_conditions name="up_down">
          <surface_ids>
            <integer_value shape="2" rank="1">9 10</integer_value>
          </surface_ids>
          <type name="dirichlet">
            <apply_weakly/>
            <align_bc_with_cartesian>
              <x_component>
                <constant>
                  <real_value rank="0">0</real_value>
                </constant>
              </x_component>
              <y_component>
                <constant>
                  <real_value rank="0">0</real_value>
                </constant>
              </y_component>
            </align_bc_with_cartesian>
          </type>
        </boundary_conditions>
        <boundary_conditions name="right_left">
          <surface_ids>
            <integer_value shape="2" rank="1">7 8</integer_value>
          </surface_ids>
          <type name="dirichlet">
            <apply_weakly/>
            <align_bc_with_cartesian>
              <x_component>
                <constant>
                  <real_value rank="0">0</real_value>
                </constant>
              </x_component>
              <y_component>
                <constant>
                  <real_value rank="0">0</real_value>
                </constant>
              </y_component>
            </align_bc_with_cartesian>
          </type>
        </boundary_conditions>
        <vector_field name="Absorption" rank="1">
          <diagnostic>
            <mesh name="PressureMesh_Discontinuous"/>
            <algorithm name="Internal" material_phase_support="multiple"/>
            <stat>
              <include_in_stat/>
            </stat>
            <convergence>
              <include_in_convergence/>
            </convergence>
            <detectors>
              <include_in_detectors/>
            </detectors>
            <steady_state>
              <include_in_steady_state/>
            </steady_state>
          </diagnostic>
          <default_absorption/>
        </vector_field>
        <detectors>
          <include_in_detectors/>
        </detectors>
        <consistent_interpolation/>
        <output/>
      </prognostic>
    </vector_field>
    <scalar_field name="PhaseVolumeFraction" rank="0">
      <prognostic>
        <mesh name="PressureMesh"/>
        <initial_condition name="WholeMesh">
          <constant>
            <real_value rank="0">1</real_value>
          </constant>
        </initial_condition>
        <detectors>
          <include_in_detectors/>
        </detectors>
        <consistent_interpolation/>
      </prognostic>
    </scalar_field>
    <scalar_field name="Concentration" rank="0">
      <prognostic>
        <mesh name="PressureMesh"/>
        <Impose_min_max>
          <min_limit>
            <real_value rank="0">0.8</real_value>
          </min_limit>
          <max_limit>
            <real_value rank="0">0.8</real_value>
          </max_limit>
        </Impose_min_max>
        <initial_condition name="WholeMesh">
          <constant>
            <real_value rank="0">0.8</real_value>
          </constant>
        </initial_condition>
        <detectors>
          <include_in_detectors/>
        </detectors>
        <consistent_interpolation/>
      </prognostic>
    </scalar_field>
    <scalar_field name="PassiveTracer_Example2" rank="0">
      <prognostic>
        <mesh name="PressureMesh"/>
        <Impose_min_max>
          <min_limit>
            <real_value rank="0">0.5</real_value>
          </min_limit>
          <max_limit>
            <real_value rank="0">0.5</real_value>
          </max_limit>
        </Impose_min_max>
        <initial_condition name="WholeMesh">
          <constant>
            <real_value rank="0">0.5</real_value>
          </constant>
        </initial_condition>
        <detectors>
          <include_in_detectors/>
        </detectors>
        <consistent_interpolation/>
      </prognostic>
    </scalar_field>
  </material_phase>
  <porous_media>
    <scalar_field name="Porosity">
      <prescribed>
        <mesh name="P0DG"/>
        <value name="WholeMesh">
          <constant>
            <real_value rank="0">0.2</real_value>
          </constant>
        </value>
        <stat/>
        <detectors>
          <exclude_from_detectors/>
        </detectors>
        <output/>
      </prescribed>
    </scalar_field>
    <scalar_field name="Permeability">
      <prescribed>
        <mesh name="P0DG"/>
        <value name="WholeMesh">
          <constant>
            <real_value rank="0">1</real_value>
          </constant>
        </value>
        <stat/>
        <detectors>
          <exclude_from_detectors/>
        </detectors>
        <output/>
      </prescribed>
    </scalar_field>
  </porous_media>
</icferst_options>
