        <initial_condition name="WholeMesh">
          <python>
            <string_value type="code" language="python" lines="20">def val(X, t):
 val = 1e-8
 if (X[0]&gt; 0.1 and X[0]&lt; 0.2): val = 1.
 return val</string_value>
          </python>
        </initial_condition>
        <boundary_conditions name="inlet">
